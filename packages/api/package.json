{
  "name": "api",
  "packageManager": "yarn@3.2.0",
  "scripts": {
    "dev": "tsnd src/index.ts"
  },
  "nodemonConfig": {
    "ignore": [
      "dist/*"
    ],
    "execMap": {
      "ts": "yarn build:dev"
    },
    "watch": [
      "src"
    ],
    "ext": "ts"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "express-fileupload": "^1.3.1",
    "heic-convert": "^1.2.4",
    "nanoid": "^3.3.1",
    "restify": "^8.6.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1.2.2",
    "@types/restify": "^8.5.4",
    "nodemon": "^2.0.15",
    "ts-node": "^10.6.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.2"
  }
}
